@using SimpleCsApp.Models
@model IEnumerable<Product>
@{ Layout = null; }

<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Simple C# App</title>
    </head>
    <body>
        <ul>
            @foreach (Product p in Model) {
                <li>Name: @p.Name, Price: @p.Price</li>
            }
        </ul>
    </body>
</html>